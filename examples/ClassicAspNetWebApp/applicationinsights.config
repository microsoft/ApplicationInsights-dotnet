<?xml version="1.0" encoding="utf-8"?>
<ApplicationInsights xmlns="http://schemas.microsoft.com/ApplicationInsights/2013/Settings">
  <!-- 
    CORE CONFIGURATION (Required)
    Replace with your actual Application Insights connection string
  -->
  <ConnectionString>InstrumentationKey=00000000-0000-0000-0000-000000000000;IngestionEndpoint=https://your-ingestion-endpoint.applicationinsights.azure.com/</ConnectionString>

  <!-- 
    SAMPLING & RATE LIMITING
  -->
  <SamplingRatio>1.0</SamplingRatio>
  <TracesPerSecond>5.0</TracesPerSecond>
  <EnableTraceBasedLogsSampler>true</EnableTraceBasedLogsSampler>

  <!-- 
    STORAGE CONFIGURATION
  -->
  <StorageDirectory>C:\temp\applicationinsights</StorageDirectory>
  <DisableOfflineStorage>false</DisableOfflineStorage>

  <!-- 
    FEATURE FLAGS
  -->
  <DisableTelemetry>false</DisableTelemetry>
  <EnableQuickPulseMetricStream>true</EnableQuickPulseMetricStream>
  <EnablePerformanceCounterCollectionModule>true</EnablePerformanceCounterCollectionModule>
  <AddAutoCollectedMetricExtractor>true</AddAutoCollectedMetricExtractor>
  <EnableDependencyTrackingTelemetryModule>true</EnableDependencyTrackingTelemetryModule>
  <EnableRequestTrackingTelemetryModule>false</EnableRequestTrackingTelemetryModule>
  <ApplicationVersion>1.0.0</ApplicationVersion>
</ApplicationInsights>
